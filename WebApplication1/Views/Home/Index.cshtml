@{
    ViewData["Title"] = "Home - ";

}

@model landingPageModel
    <!-- HOME -->
<section id="home">
    <div class="row">
        <div class="owl-carousel owl-theme home-slider">
            @foreach (var home in Model.Home)
            {
                <div class="item" style="background-image: url(../@home.Image);">
                    <div class="caption">
                        <div class="container">
                            <div class="col-md-6 col-sm-12">
                                @Html.Raw(home.Text)
                                @Html.Raw(home.Button)
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</section>

<!-- FEATURE -->
<section id="feature">
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-sm-4">
                <div class="feature-thumb">
                    <span><i class="fas fa-infinity"></i></span>
                    <h3>IT Solution</h3>
                    <p style="text-align:justify;">Small and mid-sized businesses need to be efficient to be competitive. At Tentakel, we deal in efficiency. We provide comprehensive IT Consulting services to companies that are looking to leverage their company's IT into higher degrees of productivity, and ultimately, profitability.</p>
                </div>
            </div>

            <div class="col-md-4 col-sm-4">
                <div class="feature-thumb">
                    <span><i class="fas fa-industry"></i></span>
                    <h3>Automation Solution</h3>
